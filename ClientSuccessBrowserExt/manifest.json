{
  "manifest_version": 2,

  "name": "TT Client Success",
  "description": "Client success plugin for Traction Tools",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
   "default_icon": "gray.png",
   "default_title": "Traction Tools Client Success"
  }, 
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "tabs",   
    "nativeMessaging"
   ],
   "web_accessible_resources": [
        "inject.css",		
        "jquery.js"
    ]  
}