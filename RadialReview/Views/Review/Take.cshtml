@model List<RadialReview.Models.AnswerModel>

@{
    ViewBag.Title = "Review";
    Layout = "~/Views/Shared/_ReviewLayout.cshtml";
}

@using RadialReview.Models

@Html.Partial("Slider", Model.Where(x => x is SliderAnswer)     .Cast<SliderAnswer>().ToList())
@Html.Partial("Thumbs", Model.Where(x => x is ThumbsAnswer)     .Cast<ThumbsAnswer>().ToList())
@Html.Partial("Feedback", Model.Where(x => x is FeedbackAnswer) .Cast<FeedbackAnswer>().ToList())




