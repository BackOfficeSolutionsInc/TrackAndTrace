@model RadialReview.Models.L10.L10Recurrence.L10Recurrence_Page

<input type="hidden" name ="Id" id="Id" value="@Model.Id" />
@Html.Hidden("L10RecurrenceId", Model.L10RecurrenceId)
@Html.HiddenFor(x => x.Subheading)
@Html.HiddenFor(x => x.DeleteTime)

<div class="row smallBreak2">
	<div class="col-sm-3"><label>Page Type:</label></div>
	<div class="col-sm-9">@Html.EnumDropDownListFor(x => x.PageType, new { @class = "fullWidth form-control blend" })</div>
</div>
<div class="row smallBreak2">
	<div class="col-sm-3"><label>Title:</label></div>
	<div class="col-sm-9">@Html.TextBoxFor(x => x.Title, new { @class = "fullWidth form-control blend" })</div>
</div>
<div class="row smallBreak2">
	<div class="col-sm-3"><label>Duration (Minutes):</label></div>
	<div class="col-sm-9">@Html.TextBoxFor(x => x.Minutes, new { @class = "fullWidth form-control blend" })</div>
</div>
