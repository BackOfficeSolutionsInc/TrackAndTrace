@model RadialReview.Controllers.RocksController.RockVM


<div class="form">
	@Html.HiddenFor(x => x.UserId)
	@{
		ViewBag.HideDelete = true;
		ViewBag.HidePeriod = true;
		ViewBag.HideUser = true;
		
	}
	<div class="">

		<div class="row">
			<div class="col-sm-10 col-sm-offset-1" id="allRows">
				<div>
					<label>Edit @Html.Organization().Settings.RockName for @(((List<SelectListItem>)ViewBag.Periods).First().Text):</label>
				</div>
				@foreach (var item in Model.Rocks)
				{
					Html.RenderPartial("_RockRow", item);
				}
			</div>
		</div>
        <div>
            <label>Update All Level 10s: </label> @Html.CheckBoxFor(x => x.UpdateAllL10s)
        </div>
	</div>
</div>
<script>
</script>

