@model RadialReview.Models.OrganizationModel

<div style="text-align:center">
    <div class="alignCenter fullWidth" style="display:inline-block" data-id="@Model.Id">
        <div class="thumbnaill fullWidth" style="min-height:256px;">
            <div class="imageContainer id_@Model.Id">
                <img alt="@Model.GetName()" onload="this.className=this.className.replace('fixsizeimage', '' );" class="thumbnail " style="text-align: center; margin-left: auto; margin-right: auto; max-width: 100%;" src="@Model.GetImageUrl()" />
            </div>
            <div class="caption  id_@Model.Id">
                <h3>@Model.GetName()</h3>
            </div>
        </div>
    </div>
</div>