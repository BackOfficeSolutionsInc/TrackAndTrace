					</td>*@
				<td>
					@Html.DisplayFor(modelItem => item.ImplementerName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Website)
				</td>
				@*<td>
						@Html.DisplayFor(modelItem => item.PaymentPlan)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.EosStartedAgo)
					</td>*@
				@*<td>
						@Html.DisplayFor(modelItem => item.Address_1)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Address_2)
					</td>*@

				@*<td>
						@Html.DisplayFor(modelItem => item.DisableEdit)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Guid)
					</td>*@
				<td>
					@Html.DisplayFor(modelItem => item.EosStartTime)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.OrganizationId)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.UserId)
				</td>
				@*<td>
						@Html.DisplayFor(modelItem => item.DeleteTime)
					</td>*@
				@*<td>
						@Html.DisplayFor(modelItem => item.ContactCompleteTime)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.OutcomeCompleteTime)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.L10CompleteTime)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.CreditCardCompleteTime)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.CreateOrganizationTime)
					</td>*@
				<td>
					@Html.DisplayFor(modelItem => item.City)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.State)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Zip)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Country)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.UserAgent)
				</td>
				@*<td>
						@Html.DisplayFor(modelItem => item.Languages)
					</td>*@
			</tr>
		}

	</table>
</div>

