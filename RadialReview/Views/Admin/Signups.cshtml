					@Html.DisplayNameFor(model => model.DeleteTime)
				</th>*@
			@*<th>
						@Html.DisplayNameFor(model => model.ContactCompleteTime)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.OutcomeCompleteTime)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.L10CompleteTime)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.CreditCardCompleteTime)
					</th>
				<th>
					@Html.DisplayNameFor(model => model.CreateOrganizationTime)
				</th>*@
			<th>
				@Html.DisplayNameFor(model => model.City)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.State)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Zip)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Country)
			</th>
			<th>
					@Html.DisplayNameFor(model => model.UserAgent)
				@*</th>
				<th>
					@Html.DisplayNameFor(model => model.Languages)
				</th>*@
		</tr>

		@foreach (var item in Model.Where(x=> !string.IsNullOrWhiteSpace(x.Email) || !string.IsNullOrWhiteSpace(x.LastName) || !string.IsNullOrWhiteSpace(x.FirstName) || !string.IsNullOrWhiteSpace(x.Phone))) {
			<tr>
				@*<td>
						@Html.DisplayFor(modelItem => item.CurrentPage)
					</td>*@
				<td>
					@Html.DisplayFor(modelItem => item.StartTime)
				</td>
				<td>
					@if (item.CreateOrganizationTime > new DateTime(1902, 1, 1)) {
						@Html.DisplayFor(modelItem => item.OrganizationCompleteTime)
					}
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.FirstName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.LastName)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Email)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Phone)
				</td>
