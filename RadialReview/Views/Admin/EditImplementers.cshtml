@model RadialReview.Models.Application.Coach

@{
    Layout = null;
}


@Html.HiddenFor(x => x.Id)
@Html.HiddenFor(x => x.UserOrgId)
@Html.HiddenFor(x => x.CreateTime)
@Html.HiddenFor(x => x.DeleteTime)

<div class="form-horizontal">
	<div class="form-group">
		<label class="col-sm-2 control-label" for="Name">Name</label>
		<div class="col-sm-10">
			@Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="Email">Email</label>
		<div class="col-sm-10">
			@Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="Email">Type</label>
		<div class="col-sm-10">
			@Html.EnumDropDownListFor(x => x.CoachType, new { @class = "form-control" })
		</div>
	</div>
</div>