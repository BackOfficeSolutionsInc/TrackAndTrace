@model long
<div class="review smallBreak2 loading tiny " style2="min-height:30px;">
    <div class="scorecard-heading heading" style="@(ViewBag.HeadingStyle)" data-style="@(ViewBag.HeadingStyle)">
        {{(model.L10Scorecards | filter:{KeyId:@(Model)})[0].Title}}<a href="/l10/details/@(Model)#Scorecard" class=""> (View all)</a>

    </div><div class="l10-scorecard-tile"><div class="specialDiv tile hidden-width-1" fix-head-scroller="top"  fix-head-parent>@{
        Html.RenderPartial("~/Views/Measurable/partial/list.cshtml", new ViewDataDictionary(){
            {"AddHeaders", false},
            {"AddController", false},
            {"ViewParent",".l10-scorecard-tile" },
            {"RecurrenceId", Model},
            {"Reorderable", true},
			//{"DisableName", true},
			{"Localization", Html.Organization().Settings.NumberFormat.Angular() },
            {"Selector", "((model.L10Scorecards)| filter:{KeyId:"+Model+"})[0].Contents "},
            { "ForceMin", ViewBag.NumberOfWeeks??13*7 }
        }); }</div><div class="tile visible-width-1">@{
Html.RenderPartial("~/Views/Scorecard/Partial/tile.cshtml", new ViewDataDictionary(){
            {"AddHeaders",false},
            {"AddController",false},
            {"DisableName",true},
            {"ViewParent",".l10-scorecard-tile" },
            {"RecurrenceId", Model},
            {"Selector", "((model.L10Scorecards)| filter:{KeyId:"+Model+"})[0].Contents "},
        });}</div><div ng-show="(model.L10Scorecards | filter:{KeyId:@(Model)}).length==0" class="gray alignCenter" elem-ready="functions.reload((model.L10Scorecards | filter:{KeyId:@(Model)}).length==0)"></div></div></div>

