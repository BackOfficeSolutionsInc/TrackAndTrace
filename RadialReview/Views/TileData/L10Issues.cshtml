@model long

<div class="review smallBreak2 loading tiny l10-issue-tile" style="min-height:30px">
    <div class="l10-issue-heading heading" style="@(ViewBag.HeadingStyle)">
        {{(model.L10Issues | transform:this | filter:{KeyId:@(Model)})[0].Title}}<a href="/l10/details/@(Model)#Issues" class=""> (View all)</a>
        <a href="/todo/list" class="gray"></a>
    </div><div class="tile">@{ Html.RenderPartial("~/Views/Issues/Partial/list.cshtml", new ViewDataDictionary(){
		{"AddHeaders", false},
		{"AddController", false},
		{"Selector", "((model.L10Issues | transform:this)| filter:{KeyId:"+Model+"})[0].Contents "},
	}); }<div ng-show="(model.L10Issues | transform:this | filter:{KeyId:@(Model)}).length==0" class="gray alignCenter" elem-ready="functions.reload((model.L10Issues | transform:this | filter:{KeyId:@(Model)}).length==0)">Please wait</div></div>
</div>

