@model long
@{
	ViewBag.Title = "To-do List";
	Layout = "~/Views/Shared/_Layout.cshtml";
	var dict = new ViewDataDictionary(ViewBag);
	dict.Add(new KeyValuePair<string, object>("SetName","All To-Dos"));
}
@Html.Partial("~/Views/Todo/Partial/list.cshtml")
@Html.Partial("~/Views/Todo/Partial/listscripts.cshtml",Model,dict)

@using (Html.BeginStyles()){
	<style>
		.height-placeholder {
			min-height: 60px;
		}

		.component {
			margin: 0px;
		}
	</style>
}