							<div class="organzation-name"><span class="title">NAME:</span><input ng-change="functions.sendUpdate((model| transform:this))" ng-model="(model| transform:this).Name " type="text" ng-model-options="{ debounce: 250 }" > </div>
							<div class="page-title">VISION</div>
						</div>
						<div class="col-lg-1"></div>
					</div>
					<div class="clearfix"></div>
				</div>
