													<ol>
														<li ng-repeat="value in (model| transform:this).Values">
															<div ng-if="value.Deleted===false">
																<input ng-change="functions.sendUpdate(value)" ng-model="value.CompanyValue" />
																<span class="delete glyphicon glyphicon-remove" ng-click="functions.Get('/VTO/DeleteCompanyValue?value='+value.Id)"></span>
															</div>
														</li>
														<li class="add-row" ng-click="functions.Get('/VTO/AddCompanyValue?vto='+vtoId)"></li>
													</ol>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
