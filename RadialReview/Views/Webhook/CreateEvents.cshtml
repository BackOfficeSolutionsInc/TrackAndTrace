@model RadialReview.Models.ViewModels.WebHookEventsViewModel

@Html.HiddenFor(m => m.Id)
<div class="form-group">
	@Html.LabelFor(x => x.Name)
	@Html.TextBoxFor(x => x.Name, new { @class = "form-control , uri", placeholder = "Enter Name", autocomplete = "off" })
</div>
<div class="form-group">
	@Html.LabelFor(x => x.Description)
	@Html.TextAreaFor(x => x.Description, new { @class = "form-control verticalOnly , des", rows = "6", placeholder = "Enter Description" })
</div>