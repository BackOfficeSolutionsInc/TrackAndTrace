@model RadialReview.Controllers.AccountabilityController.AccountabilityChartVM
@{
    ViewBag.Title = "Accountability Chart";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.FullWidth = true;
}

<div class="acc-chart">
    <div id="viewer"></div>
</div>
@using (Html.BeginStyles()) {
    @*<link href="~/Content/AccountabilityChart/AccountabilityChart.css" rel="stylesheet" />*@
    @Styles.Render("~/Content/AccountabilityChart")

}

@using (Html.BeginScripts()) {
    @*<script src="~/Scripts/d3/d3.min.js"></script>
    <script src="~/Scripts/d3/accountabilityChart.js"></script>*@
    @Scripts.Render("~/bundles/AccountabilityChart")
    <script type="text/javascript">
        $(function () {
            startAccChart("#viewer",@Model.OrganizationId);
        });
    </script>
}